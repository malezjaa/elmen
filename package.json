{
  "name": "elmen",
  "description": "Elmen is a simple boilerplate generator for ts projects",
  "version": "0.0.4",
  "license": "MIT",
  "author": "malezjaa <https://www.github.com/malezjaa>",
  "homepage": "https://www.github.com/malezjaa/elmen",
  "repository": {
    "type": "git",
    "url": "https://www.github.com/malezjaa/elmen"
  },
  "bin": {
    "elmen": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "module": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup"
  },
  "devDependencies": {
    "@types/node": "18.16.9",
    "@types/shelljs": "^0.8.15",
    "prettier": "^2.6.2",
    "tsup": "^8.0.2",
    "typescript": "~5.3.2"
  },
  "dependencies": {
    "citty": "^0.1.6",
    "consola": "^3.2.3",
    "shelljs": "^0.8.5",
    "which-pm": "^2.1.1"
  }
}
